	Series = {{AAAI}'07},
	Title = {Adaptive {Localization} in a {Dynamic} {WiFi} {Environment} {Through} {Multi}-view {Learning}},
	Url = {http://dl.acm.org/citation.cfm?id=1619797.1619824},
