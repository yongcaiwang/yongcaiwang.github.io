	Issn = {1018-4864, 1572-9451},
	Journal = {Telecommun Syst},
	Keywords = {Artificial Intelligence (incl. Robotics), Business Information Systems, Computer Communication Networks, Localization algorithms, Location accuracy, mobility, Probability Theory and Stochastic Processes, Wireless sensor networks, wireless sensor networks},
	Language = {en},
