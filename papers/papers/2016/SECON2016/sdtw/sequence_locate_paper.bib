	Doi = {10.1109/ICNSC.2014.6819631},
	File = {IEEE Xplore Abstract Record:/Users/ycw/Library/Application Support/Zotero/Profiles/ezi83ih1.default/zotero/storage/ZNUWBMEK/login.html:text/html},
	Keywords = {Accuracy, complex fading feature, Fading, hybrid radio-map, indoor radio, Lead, low variance signatures, multi-path fading effects, multiple feature radio-map location method, noise tolerant wireless indoor localization, particle filters, received signal strength signatures, RSS noises, Silicon, ubiquitous metallic architecture, Wireless networks},
	Month = apr,
	Pages = {233--238},
	Title = {Hybrid radio-map for noise tolerant wireless indoor localization},
	Year = {2014},
	Bdsk-Url-1 = {http://dx.doi.org/10.1109/ICNSC.2014.6819631}}

