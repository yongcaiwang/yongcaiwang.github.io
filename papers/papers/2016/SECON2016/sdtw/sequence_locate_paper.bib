	Issn = {1536-1233},
	Journal = {IEEE Transactions on Mobile Computing},
	Keywords = {802.11b wireless network, adaptive temporal radio map learning, deterministic model, environmental dynamics, LEMT algorithm, Location Estimation, mobile radio, model tree, online localization, probabilistic model, probability, radio direction-finding, radio networks, Reference Points, RFID-based network, Signal Strength, signal-strength-based location estimation algorithm, signal-strength value calibration, signal transmitter, static fingerprint-based technique, Temporal Radio Maps, trees (mathematics), wireless network},
